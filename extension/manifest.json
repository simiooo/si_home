{
  "manifest_version": 3,
  "name": "Si Home",
  "version": "1.0",
  "icons": {
  
  },
  "chrome_url_overrides": {
    "newtab": "home/index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "topSites",
    "favicon"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}
